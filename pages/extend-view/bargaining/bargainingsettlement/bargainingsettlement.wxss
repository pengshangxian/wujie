/* pages/extend-view//bargaining/bargainingsettlement/bargainingsettlement.wxss */
page{
  background: #fff;
}

.container {
  /* padding-bottom: 10%; */
  position: relative;
  height: 100vh;
  overflow: hidden;
}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background:linear-gradient(166deg,rgba(48,96,247,1) 0%,rgba(45,193,246,1) 100%);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-header-box .back1 {
  width: 32rpx;
  height: 44rpx;
  margin-top: 10rpx;
}

.tui-badge {
  background: #e41f19 !important;
  position: absolute;
  right: -4px;
}

.tui-icon-ml {
  margin-left: 20rpx;
}

.tui-icon {
  height: 32px;
  width: 32px;
  border-radius: 16px;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.container .red {
  width: 100%;
  height: 51rpx;
  background: #e80202;
}

.container .personifo {
  height: 102rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  width: 94%;
  background: #fff;
  margin: 0 3%;
  border-radius: 10rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  margin-top: 150rpx;
  box-shadow:0rpx 4rpx 8rpx 0rpx rgba(0,0,0,0.16);
}

.container .personifo .personifo-left {
  display: flex;
  align-items: center;
  font-size: 24rpx
}

.container .personifo .dingwei {
  width: 24rpx;
  height: 28rpx;
  margin-right: 20rpx;
}

.container .personifo .infomation{
  font-size: 24rpx;
}

.container .personifo .infomation .name{
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
}

.container .personifo .infomation .name .phone{
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
}

.container .personifo .infomation .address{
  color: #666666;
  margin-top: 10rpx;
  font-size: 24rpx;
  font-weight: 400;
}

.container .personifo .rightarrow {
  width: 10rpx;
  height: 18rpx;
}

.container .shop{
  /* height: 379rpx; */
  background: #fff;
  width: 94%;
  margin: 0 3%;
  border-radius: 10rpx;
  box-sizing: border-box;
  margin-top: 24rpx;
  font-size: 24rpx;
  overflow: hidden;
  box-shadow:0rpx 4rpx 8rpx 0rpx rgba(0,0,0,0.16);
}

.container .shop .shop-top{
  display: flex;
  height: 74rpx;
  align-items: center;
  padding: 0 20rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #333;
}

.container .shop .logo{
  width: 44rpx; 
  height: 44rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}

.container .shop .shop-mid{
  display: flex;
  padding: 0 20rpx;
  font-size: 24rpx;
  color: #333;
}

.container .shop .shop-mid .proimg{
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
}

.container .shop .shop-mid .phone{
  color: #999;
  font-size: 20rpx;
  margin: 10rpx 0 10rpx 0;
}

.container .shop .shop-mid .price{
  color: #E64048;
  font-size: 20rpx;
  font-weight: 400;
  margin-right: 20rpx;
}

.container .shop .shop-mid .fanxian{
  color: #E64048;
  font-size: 20rpx;
  padding: 0 10rpx;
  border: 1rpx solid #E64048;
  border-radius: 20rpx;
}


.container .shop .shop-bot{
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #999;
}

.container .shop .remark{
  height: 53rpx;
  margin-top: 20rpx;
  border-top: 1rpx solid rgba(238,238,238,1);
  display: flex;
  align-items: center;
  padding: 18rpx 20rpx;
  font-size: 24rpx;
  color: #333;
}

.container .shop .remark input{
  margin-top: 5rpx;
  margin-left: 10rpx;
  color: #999;
}


/* 同城自提弹框 */
.container .clickaddress-time{
  /* border-top: 1rpx solid gray; */
  /* border-bottom: 1rpx solid gray; */
  /* padding-bottom: 20rpx; */
  /* width: 400rpx;
  margin: 0 auto; */
}

.container .pick{
  background: white;
  /* padding-left: 40rpx; */
  box-sizing: border-box;
  font-size: 24rpx;
  margin: 0 auto;
  margin-left: 20rpx;
}

.container .pick picker-view-column{
  width: 33%;
  text-align: center;
  
  /* padding-bottom: 30rpx; */
}

.container .pick picker-view-column view{
  /* width: 100rpx;
  text-align: center; */
}
/* 同城自提弹框结束 */

/* 同城产品 */
.container .citypro{
  height: 178rpx;
  align-items: center;
  background: #fff;
  width: 94%;
  background: #fff;
  margin: 0 3%;
  border-radius: 20rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}

.container .citypro .citypro-top{
  font-size: 28rpx;
}

.container .citypro .citypro-mid{
    font-size: 24rpx;
    margin: 20rpx 0;
}

.container .citypro .citypro-mid image{
  width: 18rpx;
  height: 20rpx;
  margin-right: 10rpx;
}

.container .citypro .citypro-bot{
  font-size: 20rpx;
} 

.container .citypro .citypro-bot .self{
  padding: 0 10rpx;
  background: #909090;
  color: #fff;
  margin-right: 50rpx;
  border-radius: 20rpx;
}

.container .citypro .citypro-bot .delivery{
  padding: 0 10rpx;
  background: #909090;
  color: #fff;
  margin-right: 10rpx;
  border-radius: 20rpx;
}

.container .citypro .citypro-bot .selected{
  background: #E80202;
}

.container .citypro .citypro-bot .text{
  color: #626367;
}
/* 同城产品结束 */

/* 支付方式 */
.container .payment{
  height: 333rpx;
  align-items: center;
  background: #fff;
  width: 94%;
  background: #fff;
  margin: 0 3%;
  border-radius: 20rpx;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.container .payment .paymentmode{
  margin-bottom: 30rpx;
  font-size: 28rpx;
}

.container .payment image{
  width: 36rpx;
  height: 30rpx;
  vertical-align: -5rpx;
  margin-right: 10rpx;
}

.container .payment view{
  margin-bottom: 20rpx;
  font-size: 24rpx;
}

.container .payment .gou{
  width: 31rpx;
  height: 24rpx;
  float: right;
  margin-right: 40rpx;
  display: none
}

.container .payment .gou.select{
  display: block;
}
/* 支付方式结束 */

/* .container .bac{
  height: 400rpx;
  background: #E8E8E9;
} */

/* 底部支付 */
.container .orderbot{
  position: fixed;
  bottom: 0;
  height: 98rpx;
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  font-size: 24rpx;
}

.container .orderbot .pay{
  float: left;
  line-height: 98rpx;
  margin-left: 24rpx;
  box-sizing: border-box;
  color: #666;
  font-size: 24rpx;
}

.container .orderbot .pay .text1{
  color: #E64048;
  font-weight: 400;
  font-size: 45rpx;
  margin-left: 5rpx;
}

.container .orderbot .pay .text1 .money{
  color: #E64048;
  font-weight: 400;
  font-size: 24rpx;
}

.container .orderbot .pay .text1 .mian{
  color: #666;
  font-weight: 400;
  font-size: 20rpx;
  margin-left: 5rpx;
}

.container .orderbot .pay .text2{
  color: #E64048;
}

.container .orderbot .clickpay{
  float: right;
  height: 78rpx;
  line-height: 78rpx;
  width: 260rpx;
  color: #fff;
  background: #2D5DF6;
  text-align: center;
  font-size: 28rpx;
  margin-right: 16rpx;
  margin-top: 10rpx;
  border-radius: 10rpx;
}


/* 底部支付结束 */
.noaddress{
  text-align: center;
  padding: 0;
}

.noaddress image{
  width: 64rpx;
  height: 64rpx;
  margin-top: 20rpx;
  margin-bottom: 24rpx;
  text-align: center;
}

.noaddress .tips{
  margin-bottom: 34rpx;
}

.noaddress .tips view{
  color: #666666;
  font-size: 24rpx;
  font-weight: 400;
}

.noaddress .btn{
  display: flex;
  height: 80rpx;
  border-top: 1rpx solid rgba(245,245,245,1);
  color: #333333;
}

.noaddress .btn view{
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}






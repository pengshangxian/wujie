.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background:linear-gradient(166deg,rgba(48,96,247,1) 0%,rgba(45,193,246,1) 100%);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.container .red{
  height: 70rpx;
  background: #E80202;
}

.container .profit{
  width: 94%;
  height: 102rpx;
  line-height: 102rpx;
  margin: 0 3%;
  background: #fff;
  margin-top: -60rpx;
  border-radius: 10rpx;
  text-align: center;
  font-weight: 400;
  box-shadow:0rpx 4rpx 8rpx 0rpx rgba(0,0,0,0.16);
  color: #333;
  font-size: 30rpx;
}

.container .profit image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: -10rpx;
  margin-right: 7rpx;
}

.container .time{
  color: #999;
  font-size: 20rpx;
  padding: 20rpx 0;
  text-align: right;
}

.container .date {
  width: 94%;
  margin: 0 3%;
  background: #fff;
  margin-top: 20rpx;
  border-radius: 10rpx;
  padding: 19rpx 20rpx;
  box-sizing: border-box;
  box-shadow:0rpx 4rpx 8rpx 0rpx rgba(0,0,0,0.16);
}

.container .date .title {
  display: flex;
  justify-content: space-around;
  font-size: 30rpx;
  font-weight: 400;
  color: #333;
}

.container .date .title view {
  width: 100%;
  text-align: center;
}

.container .date .data {
  display: flex;
  justify-content: space-around;
  font-size: 24rpx;
  margin-bottom: 18rpx;
  color: #666666;
}

.container .date .data .money {
  color: #D34545;
}

.container .date .data view {
  width: 100%;
  text-align: center;
}

.container .date scroll-view {
  margin-top: 20rpx;
  /* height: 400rpx; */
}

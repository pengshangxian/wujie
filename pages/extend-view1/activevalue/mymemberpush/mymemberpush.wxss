page{
  /* background: #E8E8E9; */
}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background:linear-gradient(166deg,rgba(48,96,247,1) 0%,rgba(45,193,246,1) 100%);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.container .red{
  height: 70rpx;
  background: #E80202;
}

.container .profit{
  width: 94%;
  height: 102rpx;
  line-height: 102rpx;
  margin: 0 3%;
  background: #fff;
  margin-top: -60rpx;
  border-radius: 10rpx;
  text-align: center;
  font-weight: 400;
  box-shadow:0rpx 4rpx 8rpx 0rpx rgba(0,0,0,0.16);
  color: #333;
  font-size: 30rpx;
}

.container .profit image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: -10rpx;
  margin-right: 7rpx;
}

.container .time{
  color: #999;
  font-size: 20rpx;
  padding: 20rpx 0;
  text-align: right;
  margin-right: 20rpx;
}

.container .detail{
  width: 94%;
  margin: 0 3%;
  background: #fff;
  border-radius: 10rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: 400;
  box-shadow:0rpx 4rpx 8rpx 0rpx rgba(0,0,0,0.16);
  margin-top: 20rpx;
}

.container .item{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  /* border-bottom: 1rpx solid #767676; */
}

.container .detail image{
  width: 44rpx;
  height: 44rpx;
  vertical-align: -11rpx;
  border-radius: 50%;
  margin-right: 5rpx;
}

.bt{
  border-top: 1rpx solid #EEEEEE;
}
/* pages/extend-view1//inviteassemble/inviteassemble.wxss */
.container{

}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background:linear-gradient(166deg,rgba(48,96,247,1) 0%,rgba(45,193,246,1) 100%);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-header-box .back1 {
  width: 32rpx;
  height: 44rpx;
  margin-top: 10rpx;
}

.tui-badge {
  background: #e41f19 !important;
  position: absolute;
  right: -4px;
}

.tui-icon-ml {
  margin-left: 20rpx;
}

.tui-icon {
  height: 32px;
  width: 32px;
  border-radius: 16px;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.container .assemble {
  width: 93%;
  /* height: 410rpx; */
  margin: 0 3%;
  background: #fff;
  font-size: 30rpx;
  box-sizing: border-box;
  overflow: hidden;
  margin-top: 150rpx;
  border-radius: 20rpx;
  box-shadow:0px 4rpx 8rpx 0px rgba(0,0,0,0.12);
}

.container .assemble .assemble-top{
  width: 284rpx;
  height: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #EEEEEE;
  color: #666;
  margin: 18rpx auto 40rpx;
  border-radius: 10rpx;
  padding: 10rpx 0;
}


.container .assemble .assemble-mid {
  position: relative;
  text-align: center;
}

.container .assemble .assemble-mid .add {
  width: 42rpx;
  height: 42rpx;
  margin: 0 20rpx;
  vertical-align: 20rpx;
}

.container .assemble .assemble-mid image {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
}

.container .assemble .assemble-mid .assemblepeople {
  position: absolute;
  top: 0;
  left: 280rpx;
  width: 51rpx;
  height: 25rpx;
  background: #2D5DF6;
  border-radius: 10rpx;
  font-size: 18rpx;
  color: #fff;
}

.container .assemble .assemble-mid .recash {
  position: absolute;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 20rpx;
  bottom: 0rpx;
  right: -25rpx;
  color: #fff;
  background: #2D5DF6;
  padding: 0 30rpx 0 10rpx;
  border-radius: 30rpx;
  font-weight: 400;
}

.container .assemble .people{
  margin-top: 46rpx;
  color: #333;
  font-weight: 400;
  font-size: 32rpx;
  text-align: center;
  margin-bottom: 24rpx;
}

.container .assemble .invitate{
  width: 94%;
  margin: 0 3%;
  height: 70rpx;
  line-height: 70rpx;
  color: #2D5DF6;
  font-size: 28rpx;
  font-weight: 400;
  border: 1rpx solid #2D5DF6;
  border-radius: 18rpx;
  text-align: center;
}

.container .assemble .toshouye{
  width: 94%;
  margin: 0 3%;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  font-size: 28rpx;
  font-weight: 400;
  border: 1rpx solid #2D5DF6;
  border-radius: 18rpx;
  text-align: center;
  margin-top: 20rpx;
  background: #2D5DF6;
}

.container .assemble .rule{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 3%;
  margin: 20rpx 0 24rpx;
  color: #666;
  font-size: 28rpx;
  font-weight: 400;
}

.container .assemble .rule .text1{
  font-size: 24rpx;
}

.container .assemble .assemble-bot{
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 20rpx;
  color: #999;
  font-size: 20rpx;
}

.container .assemble .assemble-bot image{
  width: 24rpx;
  height: 28rpx;
  margin-right: 16rpx;
}

.baiwan{
  width: 699rpx;
  height: 174rpx;
  margin: 24rpx auto 32rpx;
}

.container .buy {
  margin: 0rpx 0 24rpx;
  text-align: center;
  font-size: 24rpx;
  color: #999;
}

/* 弹框 */
.successassemble {
  position: relative;
}

.successassemble .close {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 10rpx;
}

.successassemble .title {
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 24rpx;
  margin-bottom: 24rpx
}

.successassemble image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.successassemble .return {
  width: 110rpx;
  height: 28rpx;
  line-height: 28rpx;
  border: 1rpx solid #e64048;
  border-radius: 14rpx;
  font-size: 20rpx;
  margin: 24rpx auto;
  text-align: center;
  color: #e64048;
  font-weight: 400;
}

.successassemble .bot {
  font-size: 24rpx;
  text-align: center;
  border-top: 1rpx solid #eee;
  padding-top: 20rpx;
  color: #666;
}

.successassemble .bot .wx {
  width: 71rpx;
  height: 69rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}

.successassemble .bot .count {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-bottom: 20rpx;
  color: #333;
}

/* goods */

.container .prolist {
  display: flex;
  width: 94%;
  margin: 0 3%;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  box-sizing: border-box;
}

.container .prolist .goods {
  position: relative;
  width: 344rpx;
  background: #fff;
  margin-bottom: 20rpx;
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.12);
  border-radius: 10rpx;
}

.container .prolist .goods .imgbox {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 340rpx;
  border-radius: 10rpx;
  overflow: hidden
}

.container .prolist .goods image {
  /* width: 134rpx;
  height: 209rpx; */
  width: 100%;
  height: 100%;
}

.container .prolist .goods .content {
  padding: 20rpx;
  font-size: 20rpx;
  box-sizing: border-box;
  color: #333;
}

.container .prolist .goods .content .name {
  height: 64rpx;
  font-size: 24rpx;
  font-weight: bold;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.container .prolist .goods .content .evaluate {
  margin: 19rpx 0;
  color: #666;
}

.container .prolist .goods .content .assemble1 {
  display: flex;
  justify-content: space-between;
  margin-bottom: 30rpx;
  margin-top: 20rpx;
  font-size: 20rpx;
  color: #333;
}

.container .prolist .goods .content .assemble1 .spellback {
  border: 1rpx solid rgba(45, 93, 246, 1);
  color: #333;
  border-radius: 20rpx;
  padding-right: 10rpx;
}

.container .prolist .goods .content .assemble1 .spellback text {
  color: #fff;
  background: rgba(45, 93, 246, 1);
  border-radius: 20rpx;
  padding: 0 7rpx;
}

.container .prolist .goods .content .assemble1 .pushprofit {
  border: 1rpx solid rgba(45, 93, 246, 1);
  color: #333;
  border-radius: 20rpx;
  padding: 0 10rpx;
}

.container .prolist .goods .content .price {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  font-weight: 500;
}

.container .prolist .goods .content .money {
  color: #333;
  font-size: 32rpx;
  font-weight: bold;
}

.container .prolist .goods .content .price .price-left {
  display: flex;
  align-items: center;
}

.container .prolist .goods .content .zhu {
  width: 32rpx;
  height: 32rpx;
  color: #fff;
  background: #2d5df6;
  border-radius: 50%;
  font-size: 20rpx;
  margin-left: 10rpx;
  text-align: center;
  line-height: 32rpx;
}

.container .prolist .goods .content .price .price-right {
  color: #fff;
  height: 38rpx;
  line-height: 38rpx;
  background: rgba(45, 93, 246, 1);
  padding: 0 15rpx;
  font-size: 24rpx;
  border-radius: 20rpx;
}

.container .prolist .goods .push {
  position: absolute;
  top: 0;
  right: 0;
  width: 74rpx;
  height: 28rpx;
  background: rgba(141, 168, 255, 1);
  border-radius: 0rpx 10rpx 0rpx 10rpx;
  font-size: 18rpx;
  text-align: center;
  color: #fff;
}

.container .prolist .goods .nopush {
  position: absolute;
  top: 0;
  right: 0;
  width: 74rpx;
  height: 28rpx;
  background: rgba(45, 93, 246, 1);
  border-radius: 0rpx 10rpx 0rpx 10rpx;
  font-size: 18rpx;
  text-align: center;
  color: #fff;
}

/* goods结束 */
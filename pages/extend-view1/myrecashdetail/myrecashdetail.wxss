/* pages/mycenter-extend//extendorder/extendorder.wxss */

page {
  /* background: #e8e8e9; */
}

.container {
  position: relative;
}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background: linear-gradient(166deg, rgba(48, 96, 247, 1) 0%, rgba(45, 193, 246, 1) 100%);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-header-box .back1 {
  width: 32rpx;
  height: 44rpx;
  margin-top: 10rpx;
}

.tui-icon {
  height: 32px;
  width: 32px;
  border-radius: 16px;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.container .none {
  text-align: center;
  margin-top: 600rpx;
  color: #828181;
  font-size: 24rpx;
}

.container .none image {
  width: 124rpx;
  height: 124rpx;
  margin-bottom: 10rpx;
}

.container .top {
  width: 100%;
  position: fixed;
}

.container .top image {
  width: 18rpx;
  height: 10rpx;
  margin-top: 5rpx;
  margin-left: 10rpx;
}

.container .top {
  height: 60rpx;
}

.container .top .content {
  display: flex;
  justify-content: center;
  align-items: center;
  background: #F2F2F2;
  height: 60rpx;
  font-size: 24rpx;
  color: #333;
}

.container .top .content .time {
  background: #a6a6a8;
  border-radius: 10rpx;
  color: #fff;
  margin: 0 10rpx;
  padding: 0 5rpx;
}

.container .top .content .zhi {
  margin: 0 20rpx 0 10rpx;
}

.container .top .content .picker{
  display: flex;
  align-items: center;
  margin-left: 10rpx;
  background: #fff;
  color: #666666;
  margin-right: 10rpx;
  font-size: 20rpx;
}

.container .top .content .pickerbox{
  display: flex;
  align-items: center;
  background: #fff;
}

.tabs {
  width: 100%;
  position: relative;
  /* top: 70rpx; */
  background: #e80202;
  color: white;
  padding-top: 20rpx;
}

.tab {
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 28rpx;
}

.tab view {
  margin-bottom: 10rpx;
}

.tab .select {
  font-size: 34rpx;
}

.slider {
  height: 5rpx;
  width: 40rpx;
  background: black;
  position: absolute;
  left: 70rpx;
  background: #fff;
}

.tabs .left1 {
  left: 260rpx;
}

.tabs .left2 {
  left: 455rpx;
}

.tabs .left3 {
  left: 640rpx;
}

.container .red1 {
  background: #e80202;
  height: 60rpx;
}

.box {
  position: absolute;
  width: 94%;
  margin: 0 3%;
  top: 80rpx;
  box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.12);
}

.box .goods {
  width: 94%;
  margin: 0 3%;
  background: #fff;
  border-radius: 10rpx;
  padding: 20rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}

.box .goods .goods-top {
  font-size: 20rpx;
  color: #999999;
  margin-top: 14rpx;
}

.box .goods .goods-top .time {
  display: flex;
  justify-content: space-between;
  margin: 5rpx 0;
}

.box .goods .goods-top .state {
  display: flex;
  justify-content: space-between;
}

.box .goods .goods-mid {
  display: flex;
  align-items: center;
  font-size: 20rpx;
  color: #999999;
  border-bottom: 1rpx solid #EEEEEE;
  padding-bottom: 20rpx;
}

.box .goods .goods-mid .name {
  color: #333333;
  margin-bottom: 5rpx;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.box .goods .goods-mid .name text {
  background: #D54E4E;
  color: #fff;
  border-radius: 20rpx;
  padding: 0 10rpx;
  font-size: 20rpx;
}

.box .goods .goods-mid .count {
  margin: 5rpx 0;
}

.box .goods .goods-mid .money {
  display: flex;
  justify-content: space-between;
}

.box .price {
  color: #e80202;
}

.box .goods .goods-mid image {
  width: 140rpx;
  height: 140rpx;
  margin-right: 30rpx;
}

.box .goods  .goods-bot {
  font-size: 20rpx;
  color: #0257e1;
  margin-top: 10rpx;
  text-align: right;
}

/* 底部弹出框 */
.container .showtext{
  /* padding: 20rpx 20rpx 40rpx 20rpx； */
}

.container .showtext .title{
  color: #333333;
  text-align: center;
  margin-top: 30rpx;
  margin-bottom: 10rpx;
  font-size: 28rpx;
}

.container .showtext .text{
  font-weight: 400;
  font-size: 24rpx;
  padding: 0 40rpx 38rpx;
  color: #666;
}

.container .showtext .text view{
  line-height: 40rpx;
  text-indent: 20rpx;
}

.container .showtext .know{
  width: 100%;
  height: 80rpx;
  font-size: 28rpx;
  background: #2D5DF6;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  border-radius: 0 0 10rpx 10rpx;
}


/* 时间弹框 */

.container .clicktime {
  position: relative;
}

.container .clicktime .time {
  border-top: 1rpx solid #a6a6a8;
  border-bottom: 1rpx solid #a6a6a8;
  margin-top: 127rpx;
}

.container .pick {
  background: white;
  padding-left: 40rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  margin: 0 auto;
  margin-left: 30rpx;
}

.container .pick .picker-view-column {
  text-align: center;
  padding-bottom: 30rpx;
}

.container .clicktime .btn {
  display: flex;
  justify-content: space-between;
  margin-top: 85rpx;
  font-size: 30rpx;
}

.container .clicktime .btn .cancle {
  width: 155rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid #828181;
  border-radius: 35rpx;
  color: #828181;
}

.container .clicktime .btn .submit {
  width: 155rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #ea0104;
  border-radius: 35rpx;
  color: #fff;
}

.container .clicktime .x {
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}

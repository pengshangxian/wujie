.table {
  width: 94%;
  margin: 0 3%;
  /* margin-top: -50rpx; */
  border-radius: 10rpx;
  box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.12);
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 80rpx;
  align-items: center;
}

.td {
  width: 30%;
  justify-content: center;
  text-align: center;
  color: #666666;
  font-size: 24rpx;
}

.bg-w {
  /* background: snow; */
}

.bg-g {
  background: #fff;
}

.th {
  width: 30%;
  justify-content: center;
  background: #fff;
  color: #333;
  display: flex;
  height: 9rpx;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
}

.th1 {
  width: 40%;
}

.td1 {
  width: 40%;
  color: #000;
}

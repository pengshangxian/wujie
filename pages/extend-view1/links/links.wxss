  margin-right: 30rpx;
}

.container .pro .pro-top .name {
  /* margin-bottom: 40rpx; */
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.container .pro .pro-top .spec{
  color: #999999;
  font-size: 20rpx;
}

.container .pro .pro-top .money {
  display: flex;

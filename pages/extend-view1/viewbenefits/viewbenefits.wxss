/* pages/extend-view1//viewbenefits/viewbenefits.wxss */

.container {
  height: 100vh;
  overflow: hidden;
}

.container image {
  width: 100%;
  height: 86%;
}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background: #fff;
  overflow: hidden;
  color: #333;
  box-shadow: 0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.12);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-header-box .back1 {
  width: 32rpx;
  height: 44rpx;
  margin-top: 10rpx;
}

.tui-badge {
  background: #e41f19 !important;
  position: absolute;
  right: -4px;
}

.tui-icon-ml {
  margin-left: 20rpx;
}

.tui-icon {
  height: 32px;
  width: 32px;
  border-radius: 16px;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.zhijie {
  position: absolute;
  width: 94%;
  height: 84rpx;
  border-radius: 42rpx;
  background: rgba(253, 228, 6, 1);
  box-shadow: 0px 6rpx 12rpx 0px rgba(253, 192, 6, 0.58);
  bottom: 30rpx;
  text-align: center;
  line-height: 84rpx;
  margin: 0 3%;
  font-size: 32rpx;
  color: #333333;
}

.zhijie1{
  background:#FFF8B9;
  color: #999999;
}

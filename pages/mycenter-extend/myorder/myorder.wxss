.container .ordercancle {
}

.container .ordercancle .jz {
  width: 72rpx;
  height: 72rpx;
  padding-left: 220rpx;
}

.container .ordercancle .tips {
  width: 311rpx;
  margin: 0 auto;
  font-size: 28rpx;
  text-align: center;
  /* margin-top: 20rpx; */
  line-height: 40rpx;
  color: #000;
  font-weight: bold;
}

.container .ordercancle .content {
  height: 70rpx;
  font-size: 24rpx;
  color: #3e3e3e;
  margin-top: 20rpx;
  text-align: center;
}

.container .ordercancle .btn {
  font-size: 30rpx;
  text-align: center;
  color: #e80202;
  font-weight: bold;
  margin-top: 20rpx;
}

/* 取消订单的弹框结束 */

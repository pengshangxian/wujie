.container .address {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 94%;
  margin: 0 3%;
  height: 107rpx;
  background: #fff;
  border-radius: 10rpx;
  font-size: 20rpx;
  padding: 20rpx 0;
  box-sizing: border-box;
  color: #666666;
}

.container .address .address-top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
}

.container .address .modify {
  width: 70rpx;
  height: 44rpx;
  line-height: 44rpx;
  border: 1rpx solid rgba(232, 2, 2, 1);
  color: rgba(232, 2, 2, 1);
  border-radius: 10rpx;
  text-align: center;
  font-weight: bold;
}

.container  .address image {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}

.container  .orderinfo-top .ordernum{
  /* width: 590rpx; */
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
}

.container  .orderinfo-top .ordernum .copy{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 60rpx;
  height: 30rpx;
  border-radius: 20rpx;
  color: #2D5DF6;
  border: 1rpx solid #2D5DF6;
  margin-left: 50rpx;
  padding: 0 5rpx;
}

/* 商品详情 */

.container .content {
  width: 94%;
  margin: 0 3%;
  border-radius: 10rpx;
  background: #fff;
  overflow: hidden;
}

.container .content .shop {
  height: 74rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  margin-bottom: 5rpx;
}

.container .content .shop image {
  height: 46rpx;
  width: 46rpx;
  /* padding: 0 20rpx; */
  margin-right: 20rpx;
}

.container .content .content-mid {
  /* height: 100%;
  flex-direction: column;
  justify-content: space-between; */
  padding: 0 0 10rpx 0;
  display: flex;
  font-size: 24rpx;
  border-bottom: 1rpx solid #EEEEEE;
}

.container .content .content-mid image {
  width: 180rpx;
  height: 180rpx;
  margin-right: 20rpx;
}

.container .content .content-mid .model {
  font-size: 24rpx;
  /* margin-top: 10rpx; */
  color: #999999;
}

.container .content .content-mid .count {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  color: #999999;
  /* margin-top: 10rpx; */
}

.container .content .content-mid .info{
  width: 460rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 28rpx;
}

.container .content .content-mid .price {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #333333;
  padding-bottom: 1rpx;
}

.container .content .content-mid .price>view:first-child{
  font-size: 24rpx;
  color: #333333;
}

.container .content .content-mid .price>view:last-child{
  display: flex;
  align-items: center;
}

.container .content .content-mid .price .recash{
  border: 1rpx solid #2D5DF6;
  color: #2D5DF6;
  font-size: 20rpx;
  padding: 0 10rpx;
  border-radius: 20rpx;
}

.container .content .content-mid .price .returnmoney{
  border: 1rpx solid #2D5DF6;
  color: #fff;
  background: #2D5DF6;
  font-size: 20rpx;
  padding: 0 10rpx;
  border-radius: 20rpx;
  margin-left: 30rpx;
}

.container .content .assemble {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 76rpx;
  font-size: 24rpx;
  padding:20rpx 0;
  box-sizing: border-box;
  border-bottom: 1rpx solid #EEEEEE;
  color: #999999;
}

.container .content .assemble .paytext {
  color: #999999;
}

.container .content .assemble .price {
  color: #E64048;
  margin-left: 20rpx;
  margin-right: 10rpx;
}

.container .content .assemble .recash {
  width: 122rpx;
  height: 44rpx;
  line-height: 44rpx;
  border-radius: 20rpx;
  text-align: center;
  background: #2D5DF6;
  color: #fff;
  margin-left: 30rpx;
  font-size: 20rpx;
  padding: 0 10rpx;
}

/* 商品详情结束 */

.container .orderinfo {
  width: 94%;
  /* height: 199rpx; */
  margin: 0 3%;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.12);
  margin-top: 20rpx;
}

.container .orderinfo .orderinfo-top {
  padding: 20rpx 0;
  margin: 0 3%;

}

.container .orderinfo .orderinfo-top view {
  display: flex;
  justify-content: space-between;
}

.container .orderinfo .orderinfo-top view .bor {
  border: 1rpx solid #626367;
  border-radius: 10rpx;
  padding: 0 10rpx;
}

.container .orderinfo .orderinfo-bot {
  padding: 30rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 24rpx;
}

.container .orderinfo .orderinfo-bot view{
  display: flex;
  align-items: center;
  color: #2D5DF6;
}

.container .orderinfo .orderinfo-bot image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 10rpx;
}

.container .tuijian {
  font-size: 24rpx;
  text-align: center;
  margin-top: 20rpx;
}

.container .bot {
  width: 100%;
  height: 82rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: fixed;
  bottom: 0;
  background: #fff;
  font-size: 24rpx;
  font-weight: bold;
  box-shadow:0px -4rpx 12rpx 0px rgba(0,0,0,0.12);
}

.container .bot .hurry {
  width: 182rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid #999999;
  border-radius: 10rpx;
  margin-right: 20rpx;
  text-align: center;
  color: #666666;
}

.container .bot .again{
  width: 184rpx;
  height: 62rpx;
  line-height: 62rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  text-align: center;
  background: #2D5DF6;
  color: #fff;
}


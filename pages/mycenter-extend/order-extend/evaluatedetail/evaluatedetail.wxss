/* pages/mycenter-extend//order-extend/evaluatedetail/evaluatedetail.wxss */

page {
  /* background: #e8e8e9; */
}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background: linear-gradient(166deg, rgba(48, 96, 247, 1) 0%, rgba(45, 193, 246, 1) 100%);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-header-box .back1 {
  width: 32rpx;
  height: 44rpx;
  margin-top: 10rpx;
}

.tui-icon {
  height: 32px;
  width: 32px;
  border-radius: 16px;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.proinfo {
  width: 94%;
  margin: 0 3%;
  box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.12);
}

.container .logistics {
  width: 94%;
  margin: 0 3%;
  /* height: 249rpx; */
  background: #fff;
  margin-top:20rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
  box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.12);
}

.container .logistics .logistics-top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* height: 150rpx; */
  padding: 20rpx;
  box-sizing: border-box;
  color: #2D5DF6;
  font-size: 24rpx;
}

.container .logistics .logistics-top image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 28rpx;
}

.container .logistics .logistics-top .detail {
  display: flex;
  align-items: center;
}

.container .logistics .logistics-top .detail .text {
  color: #2D5DF6;
}

.container .logistics .logistics-top .right image {
  width: 10rpx;
  height: 18rpx;
}

.container .logistics .logistics-top .detail .time {
  color: #999999;
  margin-top: 10rpx;
}

.container  .address {
  display: flex;
  align-items: center;
  padding: 20rpx 20rpx 30rpx;
  color: #666666;
  font-size: 24rpx;
}

.container  .address image {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}

.container  .orderinfo-top .ordernum{
  /* width: 590rpx; */
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.container  .orderinfo-top .ordernum .copy{
  width: 60rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius: 20rpx;
  color: #2D5DF6;
  border: 1rpx solid #2D5DF6;
  padding: 0 5rpx;
}

/* 商品详情 */

.container .content {
  width: 94%;
  margin: 0 3%;
  border-radius: 10rpx;
  background: #fff;
  overflow: hidden;
  color: #333;
}

.container .content .shop {
  height: 74rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  margin-bottom: 5rpx;
}

.container .content .shop image {
  height: 46rpx;
  width: 46rpx;
  margin-right: 20rpx;
}

.container .content .content-mid {
  padding: 0 0 10rpx;
  display: flex;
  font-size: 24rpx;
  border-bottom: 1rpx solid #EEEEEE;
}

.container .content .content-mid image {
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
}

.container .content .content-mid .model {
  font-size: 24rpx;
  /* margin-top: 10rpx; */
  color: #999999;
}

.container .content .content-mid .count {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  color: #999999;
  /* margin-top: 10rpx; */
}

.container .content .content-mid .info{
  width: 460rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 28rpx;
}

.container .content .content-mid .price {
  width: 500rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #333333;
  padding-bottom: 1rpx;
}

.container .content .content-mid .price>view:first-child{
  font-size: 24rpx;
  color: #333333;
}

.container .content .content-mid .price>view:last-child{
  display: flex;
  align-items: center;
}

.container .content .content-mid .price .recash{
  border: 1rpx solid #2D5DF6;
  color: #2D5DF6;
  font-size: 20rpx;
  padding: 0 10rpx;
  border-radius: 20rpx;
}

.container .content .content-mid .price .returnmoney{
  border: 1rpx solid #2D5DF6;
  color: #fff;
  background: #2D5DF6;
  font-size: 20rpx;
  padding: 0 10rpx;
  border-radius: 20rpx;
  margin-left: 30rpx;
}

.container .content .assemble {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 76rpx;
  font-size: 24rpx;
  /* padding: 0 20rpx; */
  box-sizing: border-box;
  border-bottom: 1rpx solid #EEEEEE;
  color: #999999;
}

.container .content .assemble .paytext {
  color: #999999;
}

.container .content .assemble .price {
  color: #E64048;
  margin-left: 20rpx;
  margin-right: 10rpx;
}

.container .content .assemble .recash {
  width: 122rpx;
  height: 44rpx;
  line-height: 44rpx;
  border-radius: 20rpx;
  text-align: center;
  background: #2D5DF6;
  margin-left: 30rpx;
  color: #fff;
  font-size: 20rpx;
  padding: 0 10rpx;
}


/* 商品详情结束 */

.container .orderinfo {
  width: 94%;
  /* height: 367rpx; */
  margin: 0 3%;
  background: #fff;
  margin-top: 20rpx;
  border-radius: 10rpx;
  box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.12);
}

.container .orderinfo .orderinfo-top {
  padding: 20rpx 0;
  margin: 10rpx 3% 0;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #999999;
  border-bottom: 1rpx solid #EEEEEE;
}

.container .orderinfo .orderinfo-top>view {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 5rpx;
}

.container .orderinfo .orderinfo-top view .bor {
  width: 60rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius: 20rpx;
  color: #2D5DF6;
  border: 1rpx solid #2D5DF6;
  padding: 0 5rpx;
}

.container .orderinfo .orderinfo-bot {
  padding: 30rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 24rpx;
}

.container .orderinfo .orderinfo-bot view{
  display: flex;
  align-items: center;
  color: #2D5DF6;
}

.container .orderinfo .orderinfo-bot image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 10rpx;
}

.container .tuijian {
  font-size: 24rpx;
  text-align: center;
  margin-top: 20rpx;
}

.container .bot {
  width: 100%;
  height: 82rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: fixed;
  bottom: 0;
  background: #fff;
  font-size: 24rpx;
  font-weight: bold;
  box-shadow:0px -4rpx 12rpx 0px rgba(0,0,0,0.12);
}

.container .bot .see, .again {
  width: 182rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid #999999;
  border-radius: 10rpx;
  margin-right: 20rpx;
  text-align: center;
  color: #666666;
}

.container .bot .evaluate {
  width: 184rpx;
  height: 62rpx;
  line-height: 62rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  text-align: center;
  background: #2D5DF6;
  color: #fff;
}

/* 拨打电话的弹框 */

.container .phone {
}

.container .phone .jz {
  width: 72rpx;
  height: 72rpx;
  padding-left: 220rpx;
}

.container .phone .tips {
  width: 311rpx;
  margin: 0 auto;
  font-size: 28rpx;
  text-align: center;
  /* margin-top: 20rpx; */
  line-height: 40rpx;
  color: #000;
  font-weight: bold;
}

.container .phone .content {
  height: 70rpx;
  font-size: 24rpx;
  color: #3e3e3e;
  margin-top: 20rpx;
  text-align: center;
}

.container .phone .btn {
  font-size: 30rpx;
  text-align: center;
  color: #2D5DF6;
  font-weight: bold;
  margin-top: 20rpx;
}

/* 拨打电话的弹框结束 */

page{
  background: rgb(240,242,245)
}
.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background: #fff;
  color: #333333;
  box-shadow:0px 2px 6px 0px rgba(0,0,0,0.12);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #333333;
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-header-box .back1 {
  width: 32rpx;
  height: 44rpx;
  margin-top: 10rpx;
}

.tui-icon {
  height: 32px;
  width: 32px;
  border-radius: 16px;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.container .logobox{
  text-align: center;
  color: #333;
  font-size: 28rpx;
  font-weight: 400;
}

.container .logobox .logo{
  
}

.container .logo image{
  width: 116rpx;
  height: 116rpx;
  border-radius: 10rpx;
}

.container .logobox .name{
  margin-top: 40rpx;
  font-size: 36rpx;
  font-weight: 600;
}

.container .contentbox{
  width: 92%;
  margin: 0 4%;
  margin-top: 60rpx;
  border-top: 1rpx solid #F7F7F7;
}

.container .contentbox image{
  width: 12rpx;
  height: 24rpx;
}

.container .contentbox>view{
  height: 84rpx;
  line-height: 84rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 46rpx;
  border-bottom: 1rpx solid #F7F7F7;
  font-size: 26rpx;
  color: #333;
  font-weight: 400;
}

.container .copyright{
  color: #AAAAAA;
  font-size: 20rpx;
  text-align: center;
  margin-top: 246rpx;
}

/* 弹框 */

.container .clickaddress {
  padding: 20rpx;
  position: relative;
  color: #333;
}

.container .clickaddress .jz {
  width: 72rpx;
  height: 72rpx;
  padding-left: 220rpx;
}

.container .clickaddress .tips {
  width: 311rpx;
  margin: 0 auto;
  font-size: 34rpx;
  text-align: center;
  margin-top: 20rpx;
  line-height: 40rpx;
  margin-bottom: 16rpx;
  font-weight: 400;
}

.container .clickaddress .content{
  padding: 0 20rpx;
  box-sizing: border-box;
}

scroll-view{
  height: 600rpx;
  /* padding: 0 20rpx; */
  /* padding-left: 10rpx; */
  box-sizing: border-box;
}

.container .clickaddress .btn {
  margin-top: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  background: #2D5DF6;
  color: #fff;
}

/* 弹框结束 */
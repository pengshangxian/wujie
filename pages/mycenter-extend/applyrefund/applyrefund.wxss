.tui-popup .active {
  color: #2D5DF6;
}

/* 弹框 */

.container .clickaddress {
  position: relative;
}

.container .clickaddress .jz {
  width: 72rpx;
  height: 72rpx;
  padding-left: 220rpx;
}

.container .clickaddress .tips {
  width: 311rpx;
  margin: 0 auto;
  font-size: 30rpx;
  text-align: center;
  margin-top: 60rpx;
  line-height: 40rpx;
  color: #333;
}

.container .clickaddress .btn {
  display: flex;
  margin-top: 60rpx;
  height: 60rpx;
}

.container .clickaddress .btn .cancel {
  width: 50%;
  background: #fff;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 0 0 0 10rpx;
  border-top: 1rpx solid #F5F5F5;
  font-size: 30rpx;
}

.container .clickaddress .btn .addaddress {
  width: 50%;
  color: #fff;
  background: #2D5DF6;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 0 0 10rpx 0;
  font-size: 30rpx;
}

.container .clickaddress .close {
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}

/* 弹框结束 */


/* 选择规格 */
.pro-popbox {
  padding: 30rpx 0 0 30rpx;
  position: relative;
}

.pro-popbox .x {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  right: 30rpx;
}

.popbox {
  display: flex;
}

.popbox .popup-img {
  width: 170rpx;
  height: 170rpx;
}

.popbox .popup-price {
  font-size: 26rpx;
  margin-left: 20rpx;
  padding-top: 10rpx;
}

.popbox .popup-price .price {
  font-size: 52rpx;
  font-weight: 400;
  color: #333;
}

.popbox .popup-price .price .money {
  font-size: 24rpx;
}

.popbox .popup-price .number {
  font-size: 24rpx;
  line-height: 40rpx;
  color: #999;
}

.popbox .popup-price .model {
  font-size: 24rpx;
  color: #999;
}

.choosemodel .choose {
  margin: 20rpx 0 16rpx;
  color: #999;
}

.choosemodel .content {
  display: flex;
  flex-wrap: wrap;
}

.choosemodel .model {
  margin-top: 20rpx;
  padding: 10rpx 12rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  border: 2rpx solid rgba(153, 153, 153, 1);
  color: #999;
  font-size: 20rpx;
  margin-right: 20rpx;
}

.choosemodel .model.selected {
  color: #2d5df6;
  border: 2rpx solid #2d5df6;
}

.amount1 {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 24rpx;
  margin-top: 22rpx;
  color: #999;
  font-size: 24rpx;
}

.ass-notice {
  height: 64rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #c7d5ff;
  padding-left: 30rpx;
  padding-right: 20rpx;
  font-size: 20rpx;
  margin-top: 18rpx;
}

.ass-notice .notice-left {
  display: flex;
  align-items: center;
}

.ass-notice view {
  height: 100%;
  line-height: 64rpx;
}

.ass-notice image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 20rpx;
}

.ass-notice .people {
  color: #e80202;
  font-weight: 700;
}

.ass-notice .quick {
  color: #2d5df6;
}

.popbox-bot {
  display: flex;
  align-items: center;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.popbox-bot image {
  width: 42rpx;
  height: 42rpx;
}

.popbox-bot {
  margin-top: 100rpx;
  margin-bottom: 10rpx;
}

.popbox-bot .addaddress {
  width: 26%;
  text-align: center;
}

.popbox-bot .buying {
  width: 260rpx;
  height: 78rpx;
  line-height: 78rpx;
  background: #c7d5ff;
  color: #2d5df6;
  border-radius: 10rpx;
  text-align: center;
  font-size: 28rpx;
  margin-right: 10rpx;
}

.popbox-bot .assembling {
  width: 260rpx;
  height: 78rpx;
  line-height: 78rpx;
  background: #2d5df6;
  color: #fff;
  border-radius: 10rpx;
  text-align: center;
  font-size: 28rpx;
}

.addcart-determine {
  width: 94%;
  background: #2d5df6;
  margin: 0 3%;
  height: 78rpx;
  line-height: 78rpx;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
  margin-top: 40rpx;
}


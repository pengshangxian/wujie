.container {
}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background: linear-gradient(166deg, rgba(48, 96, 247, 1) 0%, rgba(45, 193, 246, 1) 100%);
}

.tui-header {
  width: 100%;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 500;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.tui-header-icon {
  position: fixed;
  left: 20rpx;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 64rpx;
  transform: translateZ(0);
  z-index: 99999;
}

.tui-header-box .back {
  width: 64rpx;
  height: 64rpx;
}

.tui-header-box .back1 {
  width: 32rpx;
  height: 44rpx;
  margin-top: 10rpx;
}

.tui-badge {
  background: #e41f19 !important;
  position: absolute;
  right: -4px;
}

.tui-icon-ml {
  margin-left: 20rpx;
}

.tui-icon {
  height: 32px;
  width: 32px;
  border-radius: 16px;
}

.tui-icon-back {
  height: 32px !important;
  width: 32px !important;
  display: block !important;
}

.container .applytop {
  width: 94%;
  margin: 0 3%;
  padding: 26rpx 28rpx 26rpx 30rpx;
  box-sizing: border-box;
  box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.12);
}

.container .applytop .goods {
  display: flex;
  font-size: 20rpx;
  color: #999;
  padding-bottom: 10rpx;
}

.container .applytop .goods .name {
  display: flex;
  font-size: 24rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.container .applytop .goods .mt {
  margin-top: 5rpx;
}

.container .applytop .goods image {
  width: 120rpx;
  height: 120rpx;
  margin-right: 30rpx;
}

.container .type {
  width: 94%;
  height: 136rpx;
  margin: 0 3%;
  background: #fff;
  border-radius: 10rpx;
  font-size: 24rpx;
}

.container .applytype image {
  width: 10rpx;
  height: 18rpx;
}

.container .applytype {
  height: 68rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 1rpx solid #eee;
  color: #666;
  font-size: 24rpx;
}

.container .applytype text {
  color: #2d5df6;
  font-size: 24rpx;
}

.container .applyreason {
  height: 68rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #666;
  font-size: 24rpx;
  border-top: 1rpx solid #eee;
}

.container .applyreason image {
  width: 10rpx;
  height: 18rpx;
}

.container .applytop .money {
  color: #666;
  font-size: 24rpx;
  border-top: 1rpx solid #eee;
  padding-top: 14rpx;
}

.container .notes {
  width: 94%;
  /* height: 297rpx; */
  margin: 0 3%;
  background: #fff;
  border-radius: 10rpx;
  font-size: 24rpx;
  margin-top: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.12);
}

.container .notes .notes-top {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  margin-bottom: 10rpx;
}

.container .notes .number {
  font-size: 20rpx;
  color: #9b9b9b;
}

.container .notes textarea {
  height: 170rpx;
}

.container .notes .upload .uploadbtn {
  width: 117rpx;
  height: 117rpx;
  line-height: 117rpx;
  border: 1rpx dotted #828181;
  text-align: center;
  color: #939393;
}

.container .notes .upload .question-images {
  display: flex;
}

.container .notes .upload .question-images>view {
  text-align: center;
  margin-right: 20rpx;
}

.container .notes .upload .q-image-wrap image {
  width: 100rpx;
  height: 100rpx;
}

.container .phone {
  display: flex;
  align-items: center;
  width: 94%;
  height: 62rpx;
  margin: 0 3%;
  background: #fff;
  border-radius: 10rpx;
  font-size: 24rpx;
  margin-top: 20rpx;
  padding: 0 20rpx;
  line-height: 62rpx;
  box-sizing: border-box;
}

.container .phone .phonenumber {
  margin-left: 30rpx;
}

.container .submit {
  width: 94%;
  height: 76rpx;
  margin: 0 3%;
  background: #2D5DF6;
  color: #fff;
  border-radius: 10rpx;
  font-size: 30rpx;
  margin-top: 80rpx;
  padding: 0 20rpx;
  line-height: 76rpx;
  box-sizing: border-box;
  text-align: center;
}

.tui-popup {
  padding-bottom: 80rpx;
}

.tui-popup .title {
  font-size: 28rpx;
  font-weight: bold;
  text-align: center;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
}

.tui-popup .tips {
  font-size: 24rpx;
  text-align: center;
  font-weight: 400;
  margin-top: 10rpx;
  color: #666666;
  margin-bottom: 19rpx;
}

.tui-popup .reason {
  width: 94%;
  margin: 0 3%;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 24rpx;
  border-top: 1rpx solid #EEEEEE;
}


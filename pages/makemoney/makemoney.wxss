  border-radius: 10rpx;
}

.material .materialbox .logo {
  width: 70rpx;
  height: 70rpx;
  vertical-align: -20rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}

.material .materialbox .name {
  font-weight: 600;

.task_number {
  color: black;
  font-size: 28rpx;
  display: flex;
  align-items: center;
}

.task_one image {
  width: 10rpx;
  height: 100rpx;
  margin-left: 0;
}

.task_purpose {
  color: #000;
  font-size: 28rpx;
  margin-left: 2%;
}

.task_one {
  width: 94%;
  margin: 0 3%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  font-weight: 500;
  color: #333;
  background: #fff;
  /* margin-top: 20rpx; */
  box-sizing: border-box;
  border-radius: 10rpx;
  padding: 6rpx 0;
  box-shadow: 0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.12);
  position: relative;
  z-index: 999;
  overflow: hidden;
}

.task_one .task_number {
  flex: 7;
  position: relative;
}

.task_one .task_number .text1 {
  margin-left: 15rpx;
}

.task_one .task_number .text2 {
  color: #2d5df6;
  font-weight: 400;
  margin-left: 5rpx;
}

.task_one .task_number .dot {
  position: absolute;
  width: 10rpx;
  height: 10rpx;
  border: 4rpx solid #2d5df6;
  border-radius: 50%;
  left: 110rpx;
  top: 30rpx;
}

.task_one .equity {
  flex: 3;
  color: #fff;
  background: #2d5df6;
  margin-left: 20rpx;
  border-radius: 20rpx;
  padding: 5rpx 10rpx;
  font-size: 24rpx;
  font-weight: 300;
  margin-right: 28rpx;
}

.task_two {
  width: 100%;
  color: #0e050a;
  font-size: 24rpx;
  font-weight: 400;
  flex: 7;
}

.new_task .content {
  width: 92%;
  margin: 0 4%;
  background: #fff;
  box-shadow: 0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.12);
  border-radius: 0px 0px 10rpx 10rpx;
  padding: 20rpx 20rpx 20rpx 30rpx;
  box-sizing: border-box;
  z-index: 990;
  overflow: hidden;
  transition: all 0.3s ease-in-out;

}

.new_task .content .task_three .task_do1 {
  background: #828181;
  font-size: 24rpx;
  color: #fff;
  padding: 5rpx 10rpx;
  border-radius: 20rpx;
}

.new_task .content .task_three .task_do2 {
  background: #2d5df6;
  font-size: 24rpx;
  color: #fff;
  padding: 5rpx 10rpx;
  border-radius: 20rpx;
}

.new_task .content .task_three .btn {
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: #2d5df6;
  font-size: 24rpx;
  padding: 0;
  margin: 0;
}

.task_three {
  display: flex;
  justify-content: flex-end;
  /* width: 15%;
  height: 40rpx;
